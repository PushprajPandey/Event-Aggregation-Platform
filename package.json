{
  "name": "sydney-events-aggregator",
  "version": "1.0.0",
  "description": "Sydney Events Aggregation Platform - Backend API",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "rimraf dist && npm run build",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "deploy:build": "npm ci --only=production && npm run build:clean",
    "deploy:start": "npm run start:prod"
  },
  "keywords": [
    "events",
    "scraping",
    "sydney",
    "aggregator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^7.0.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.4",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.0",
    "@types/node-cron": "^3.0.8",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/supertest": "^2.0.12",
    "fast-check": "^3.12.0",
    "jest": "^29.6.2",
    "rimraf": "^5.0.10",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  }
}
